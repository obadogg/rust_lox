    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/lodash": "^4.14.176",
    "antd": "^4.16.13",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
